{
  "name": "next-ts",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "yarn generate && next dev",
    "generate": "prisma generate && yarn codegen",
    "build:nexus-typegen": "ts-node --compiler-options '{\"module\":\"CommonJS\"}' --transpile-only schema",
    "build": "npm run build:nexus-typegen && next build",
    "start": "next start",
    "codegen": "graphql-codegen --config codegen.yml",
    "prisma:seed": "ts-node prisma/seed.ts"
  },
  "dependencies": {
    "@apollo/client": "3.5.7",
    "@prisma/client": "3.8.1",
    "apollo-server-micro": "3.6.1",
    "graphql": "16.2.0",
    "micro": "9.3.4",
    "next": "12.0.8",
    "nexus": "1.1.0",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.3.1",
    "@types/react": "17.0.38",
    "@typescript-eslint/eslint-plugin": "5.10.0",
    "autoprefixer": "10.4.2",
    "eslint": "8.7.0",
    "eslint-config-next": "12.0.8",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "eslint-plugin-unicorn": "40.1.0",
    "eslint-plugin-unused-imports": "2.0.0",
    "postcss": "8.4.5",
    "prisma": "3.8.1",
    "prettier": "2.5.1",
    "typescript": "4.5.4",
    "tailwindcss": "3.0.15",
    "@graphql-codegen/typescript": "2.4.2",
    "@graphql-codegen/typescript-operations": "2.2.2",
    "@graphql-codegen/typescript-react-apollo": "3.2.4"
  },
  "engines": {
    "node": ">=14"
  }
}