{
  "name": "next-ts",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "yarn generate && next dev",
    "generate": "prisma generate && yarn codegen",
    "build:nexus-typegen": "ts-node --compiler-options '{\"module\":\"CommonJS\"}' --transpile-only schema",
    "build": "npm run build:nexus-typegen && next build",
    "start": "next start",
    "codegen": "graphql-codegen --config codegen.yml",
    "prisma:seed": "ts-node prisma/seed.ts"
  },
  "dependencies": {
    "@apollo/client": "3.6.2",
    "@prisma/client": "3.13.0",
    "apollo-server-micro": "3.7.0",
    "graphql": "16.4.0",
    "micro": "9.3.4",
    "next": "12.1.6",
    "nexus": "1.3.0",
    "react": "18.1.0",
    "react-dom": "18.1.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.6.2",
    "@types/react": "18.0.9",
    "@typescript-eslint/eslint-plugin": "5.23.0",
    "autoprefixer": "10.4.7",
    "eslint": "8.15.0",
    "eslint-config-next": "12.1.6",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "eslint-plugin-unicorn": "42.0.0",
    "eslint-plugin-unused-imports": "2.0.0",
    "postcss": "8.4.13",
    "prisma": "3.13.0",
    "prettier": "2.6.2",
    "typescript": "4.6.4",
    "tailwindcss": "3.0.24",
    "@graphql-codegen/typescript": "2.4.11",
    "@graphql-codegen/typescript-operations": "2.4.0",
    "@graphql-codegen/typescript-react-apollo": "3.2.14"
  },
  "engines": {
    "node": ">=14"
  }
}