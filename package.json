{
  "name": "next-ts",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "yarn generate && next dev",
    "generate": "prisma generate && yarn codegen",
    "build:nexus-typegen": "ts-node --compiler-options '{\"module\":\"CommonJS\"}' --transpile-only schema",
    "build": "npm run build:nexus-typegen && next build",
    "start": "next start",
    "codegen": "graphql-codegen --config codegen.yml",
    "prisma:seed": "ts-node prisma/seed.ts"
  },
  "dependencies": {
    "@apollo/client": "3.4.16",
    "@prisma/client": "3.3.0",
    "apollo-server-micro": "3.4.0",
    "graphql": "16.0.0",
    "micro": "9.3.4",
    "next": "11.1.2",
    "nexus": "1.1.0",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.2.2",
    "@types/react": "17.0.31",
    "@typescript-eslint/eslint-plugin": "5.1.0",
    "autoprefixer": "10.4.0",
    "eslint": "8.1.0",
    "eslint-config-next": "11.1.2",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "eslint-plugin-unicorn": "37.0.1",
    "eslint-plugin-unused-imports": "1.1.5",
    "postcss": "8.3.11",
    "prettier": "2.4.1",
    "prisma": "3.3.0",
    "tailwindcss": "2.2.19",
    "typescript": "4.4.4",
    "@graphql-codegen/typescript": "2.3.0",
    "@graphql-codegen/typescript-operations": "2.2.0",
    "@graphql-codegen/typescript-react-apollo": "3.2.0"
  },
  "engines": {
    "node": ">=14"
  }
}