{
  "name": "next-ts",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "yarn generate && next dev",
    "generate": "prisma generate && yarn codegen",
    "build:nexus-typegen": "ts-node --compiler-options '{\"module\":\"CommonJS\"}' --transpile-only schema",
    "build": "npm run build:nexus-typegen && next build",
    "start": "next start",
    "codegen": "graphql-codegen --config codegen.yml",
    "prisma:seed": "ts-node prisma/seed.ts"
  },
  "dependencies": {
    "@apollo/client": "3.7.0",
    "@prisma/client": "4.5.0",
    "apollo-server-micro": "3.10.3",
    "graphql": "16.6.0",
    "micro": "9.4.1",
    "next": "12.3.1",
    "nexus": "1.3.0",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.13.7",
    "@types/react": "18.0.21",
    "@typescript-eslint/eslint-plugin": "5.40.1",
    "autoprefixer": "10.4.12",
    "eslint": "8.25.0",
    "eslint-config-next": "12.3.1",
    "eslint-plugin-simple-import-sort": "8.0.0",
    "eslint-plugin-unicorn": "44.0.2",
    "eslint-plugin-unused-imports": "2.0.0",
    "postcss": "8.4.18",
    "prisma": "4.5.0",
    "prettier": "2.7.1",
    "typescript": "4.8.4",
    "tailwindcss": "3.1.8",
    "@graphql-codegen/typescript": "2.7.4",
    "@graphql-codegen/typescript-operations": "2.5.4",
    "@graphql-codegen/typescript-react-apollo": "3.3.4"
  },
  "engines": {
    "node": ">=14"
  }
}