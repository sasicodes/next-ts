{
  "name": "next-ts",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "yarn codegen && next dev",
    "build:nexus-typegen": "ts-node --compiler-options '{\"module\":\"CommonJS\"}' --transpile-only schema",
    "build": "npm run build:nexus-typegen && next build",
    "start": "next start",
    "codegen": "graphql-codegen --config codegen.yml",
    "prisma:seed": "ts-node prisma/seed.ts"
  },
  "dependencies": {
    "@apollo/client": "3.4.16",
    "@prisma/client": "3.2.1",
    "apollo-server-micro": "3.4.0",
    "graphql": "15.6.1",
    "micro": "9.3.4",
    "next": "11.1.2",
    "nexus": "1.1.0",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.2.1",
    "@types/react": "17.0.29",
    "@typescript-eslint/eslint-plugin": "5.0.0",
    "autoprefixer": "10.3.7",
    "eslint": "8.0.0",
    "eslint-config-next": "11.1.2",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "eslint-plugin-unicorn": "37.0.1",
    "eslint-plugin-unused-imports": "1.1.5",
    "postcss": "8.3.9",
    "prettier": "2.4.1",
    "prisma": "3.2.1",
    "tailwindcss": "2.2.17",
    "typescript": "4.4.4",
    "@graphql-codegen/typescript": "2.2.4",
    "@graphql-codegen/typescript-operations": "2.1.8",
    "@graphql-codegen/typescript-react-apollo": "3.1.6"
  },
  "engines": {
    "node": ">=14"
  }
}