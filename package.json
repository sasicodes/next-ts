{
  "name": "next-ts",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "yarn generate && next dev",
    "generate": "prisma generate && yarn codegen",
    "build:nexus-typegen": "ts-node --compiler-options '{\"module\":\"CommonJS\"}' --transpile-only schema",
    "build": "npm run build:nexus-typegen && next build",
    "start": "next start",
    "codegen": "graphql-codegen --config codegen.yml",
    "prisma:seed": "ts-node prisma/seed.ts"
  },
  "dependencies": {
    "@apollo/client": "3.7.1",
    "@prisma/client": "4.7.0",
    "apollo-server-micro": "3.11.1",
    "graphql": "16.6.0",
    "micro": "10.0.1",
    "next": "13.0.5",
    "nexus": "1.3.0",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.15.0",
    "@types/react": "18.0.27",
    "@typescript-eslint/eslint-plugin": "5.45.0",
    "autoprefixer": "10.4.13",
    "eslint": "8.28.0",
    "eslint-config-next": "13.0.5",
    "eslint-plugin-simple-import-sort": "8.0.0",
    "eslint-plugin-unicorn": "45.0.1",
    "eslint-plugin-unused-imports": "2.0.0",
    "postcss": "8.4.19",
    "prisma": "4.7.0",
    "prettier": "2.8.0",
    "typescript": "4.9.3",
    "tailwindcss": "3.2.4",
    "@graphql-codegen/typescript": "2.8.2",
    "@graphql-codegen/typescript-operations": "2.5.7",
    "@graphql-codegen/typescript-react-apollo": "3.3.7"
  },
  "engines": {
    "node": ">=14"
  }
}