{
  "name": "next-ts",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "yarn generate && next dev",
    "generate": "prisma generate && yarn codegen",
    "build:nexus-typegen": "ts-node --compiler-options '{\"module\":\"CommonJS\"}' --transpile-only schema",
    "build": "npm run build:nexus-typegen && next build",
    "start": "next start",
    "codegen": "graphql-codegen --config codegen.yml",
    "prisma:seed": "ts-node prisma/seed.ts"
  },
  "dependencies": {
    "@apollo/client": "3.6.9",
    "@prisma/client": "4.2.1",
    "apollo-server-micro": "3.10.1",
    "graphql": "16.6.0",
    "micro": "9.4.1",
    "next": "12.2.5",
    "nexus": "1.3.0",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.11.6",
    "@types/react": "18.0.17",
    "@typescript-eslint/eslint-plugin": "5.34.0",
    "autoprefixer": "10.4.8",
    "eslint": "8.22.0",
    "eslint-config-next": "12.2.5",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "eslint-plugin-unicorn": "43.0.2",
    "eslint-plugin-unused-imports": "2.0.0",
    "postcss": "8.4.16",
    "prisma": "4.2.1",
    "prettier": "2.7.1",
    "typescript": "4.7.4",
    "tailwindcss": "3.1.8",
    "@graphql-codegen/typescript": "2.7.3",
    "@graphql-codegen/typescript-operations": "2.5.3",
    "@graphql-codegen/typescript-react-apollo": "3.3.3"
  },
  "engines": {
    "node": ">=14"
  }
}