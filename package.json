{
  "name": "next-ts",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "yarn generate && next dev",
    "generate": "prisma generate && yarn codegen",
    "build:nexus-typegen": "ts-node --compiler-options '{\"module\":\"CommonJS\"}' --transpile-only schema",
    "build": "npm run build:nexus-typegen && next build",
    "start": "next start",
    "codegen": "graphql-codegen --config codegen.yml",
    "prisma:seed": "ts-node prisma/seed.ts"
  },
  "dependencies": {
    "@apollo/client": "3.5.10",
    "@prisma/client": "3.11.1",
    "apollo-server-micro": "3.6.6",
    "graphql": "16.3.0",
    "micro": "9.3.4",
    "next": "12.1.1",
    "nexus": "1.3.0",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.6.2",
    "@types/react": "17.0.43",
    "@typescript-eslint/eslint-plugin": "5.16.0",
    "autoprefixer": "10.4.4",
    "eslint": "8.11.0",
    "eslint-config-next": "12.1.1",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "eslint-plugin-unicorn": "41.0.1",
    "eslint-plugin-unused-imports": "2.0.0",
    "postcss": "8.4.12",
    "prisma": "3.11.1",
    "prettier": "2.6.1",
    "typescript": "4.6.3",
    "tailwindcss": "3.0.23",
    "@graphql-codegen/typescript": "2.4.8",
    "@graphql-codegen/typescript-operations": "2.3.5",
    "@graphql-codegen/typescript-react-apollo": "3.2.11"
  },
  "engines": {
    "node": ">=14"
  }
}